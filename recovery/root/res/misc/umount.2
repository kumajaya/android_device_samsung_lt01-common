#!/sbin/sh

echo $0 $* >> /dev/null

if (echo "$*" | grep -q -i "/system"); then
    busybox umount /system >> /dev/null 2>&1
    echo busybox umount $* >> /dev/null
elif (echo "$*" | grep -q -i "/cache"); then
    busybox umount /cache >> /dev/null 2>&1
    echo busybox umount $* >> /dev/null
elif (echo "$*" | grep -q -i "/data"); then
    echo busybox umount $* >> /dev/null
else
    busybox umount $*
    echo busybox umount $* >> /dev/null
fi;
